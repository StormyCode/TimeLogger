<UserControl x:Class="TimeLogger.Pages.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400">
    <Grid Style="{StaticResource ContentRoot}" HorizontalAlignment="Left" VerticalAlignment="Center">
        <ScrollViewer HorizontalAlignment="Left" VerticalAlignment="Center">
            <StackPanel MinWidth="200">
                <Calendar x:Name="datetimepicker" VerticalAlignment="Center" HorizontalAlignment="Left"/>
            </StackPanel>
        </ScrollViewer>
        <TextBox x:Name="txtbox_start" HorizontalAlignment="Left" Height="23" Margin="229,55,-149,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</UserControl>
